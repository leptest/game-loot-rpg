{
	"html": {
		"allowed_file_extensions": ["htm", "xhtml", "shtml", "xml", "svg", "hbs", "html"],
		"brace_style": "end-expand",
		"indent_char": "\t",
		"indent_handlebars": false,
		"indent_inner_html": true,
		"indent_scripts": "normal",
		"indent_size": 1,
		"max_preserve_newlines": 0,
		"preserve_newlines": true,
		"unformatted": ["a", "sub", "sup", "b", "strong", "i", "u", "em", "i", "u", "samp", "strike", "s", "del", "ins", "q", "cite", "mark", "kbd", "pre", "abbr", "dfn", "small", "big", "label", "span", "var", "code"],
		"wrap_line_length": 0
	},
	"css": {
		"allowed_file_extensions": ["css", "scss"],
		"end_with_newline": false,
		"indent_char": "\t",
		"indent_size": 1,
		"selector_separator": " ",
		"selector_separator_newline": false
	},
	"js": {
		"allowed_file_extensions": ["js", "json", "jshintrc", "jsbeautifyrc", "bowerrc", "prettifyrc", "csslintrc", "eslintrc", "stylelintrc"],
		"brace_style": "end-expand",
		"break_chained_methods": false,
		"e4x": false,
		"eval_code": false,
		"indent_char": "\t",
		"indent_level": 0,
		"indent_size": 1,
		"indent_with_tabs": true,
		"jslint_happy": false,
		"keep_array_indentation": false,
		"keep_function_indentation": false,
		"max_preserve_newlines": 2,
		"preserve_newlines": true,
		"space_before_conditional": true,
		"space_in_paren": false,
		"unescape_strings": false,
		"wrap_line_length": 0
	}
}
